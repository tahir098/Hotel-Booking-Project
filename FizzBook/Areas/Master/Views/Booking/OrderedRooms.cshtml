@model IEnumerable<FizzBook.Areas.Master.Models.Bookings.RoomBookingsModel>

<style>
    .svg-icon-primary .svg-icon-2x {
        margin-right: 5px;
    }

    img{
        object-fit:contain;
    }

    /* .card-body {
        border: blue;
        outline-color: #eaddf1;
        outline-style: solid;
    }*/

